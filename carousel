     <script src="
https://cdn.jsdelivr.net/npm/swiper@8.4.5/swiper-bundle.min.js
"></script>

<script>
document.addEventListener("DOMContentLoaded", function() {
    var swiper = new Swiper(".mySwiperServizi", {
        spaceBetween: 30,
        breakpoints: {
            640: {
                slidesPerView: 1,
            },
            768: {
                slidesPerView: 3,
                spaceBetween: 20
            }
        },
        on: {
            slideChangeTransitionEnd: function() {
                updateTextCarousel();
            }
        }
    });

  function setProgressBarWidth() {
        var totalSlides = swiper.slides.length;
        var progressBarWidth = 100 / totalSlides;  // calcola la larghezza in percentuale
        document.querySelector('.progressbar').style.width = progressBarWidth + '%';
    }

    function updateTextCarousel() {
        var activeDataValue = document.querySelector(".mySwiperServizi .swiper-slide-active img").getAttribute("data");
        var headingTitle = document.querySelector(".testo-carousel .elementor-heading-title");

        headingTitle.classList.remove("text-active");

        setTimeout(function() {
            document.getElementById('lista-servizi-testa').style.display = 'none';
            document.getElementById('lista-servizi-schiena').style.display = 'none';
            document.getElementById('lista-servizi-bacino').style.display = 'none';
            document.getElementById('lista-servizi-arti-inferiori').style.display = 'none';
            document.getElementById('lista-servizi-arti-superiori').style.display = 'none';

            if (activeDataValue === "Mal di Testa") {
                document.getElementById('lista-servizi-testa').style.display = 'block';
            } else if (activeDataValue === "Mal di Schiena") {
                document.getElementById('lista-servizi-schiena').style.display = 'block';
            } else if (activeDataValue === "Arti Superiori") {
                document.getElementById('lista-servizi-arti-superiori').style.display = 'block';
            } else if (activeDataValue === "Bacino") {
                document.getElementById('lista-servizi-bacino').style.display = 'block';
            } else if (activeDataValue === "Arti Inferiori") {
                document.getElementById('lista-servizi-arti-inferiori').style.display = 'block';
            }
            
            headingTitle.textContent = activeDataValue;
            headingTitle.classList.add("text-active");
        }, 220);

        // Aggiorna la progress bar
        updateProgressBar();
    }

    function updateProgressBar() {
        var activeIndex = swiper.activeIndex;
        var totalSlides = swiper.slides.length;

        var leftPercentage = (activeIndex / (totalSlides - 1)) * 100;
        var progressBar = document.querySelector('.progressbar');
        
        progressBar.style.left = leftPercentage + '%';
        progressBar.style.transform = `translateX(-${leftPercentage}%)`;
    }

    // Imposta la larghezza della stanghetta e aggiorna la progress bar al caricamento
    setProgressBarWidth();
    updateTextCarousel();
});


</script>

<style>
    
/* Definisce le proprietà base per l'animazione */
.contenitore-testo-carousel .elementor-heading-title {
    transition: opacity 0.4s;
    opacity: 0;
}

/* Classe da aggiungere con JavaScript quando il testo è "attivo" */
.text-active {
    opacity: 1!important;
}
    
</style>



    
    

